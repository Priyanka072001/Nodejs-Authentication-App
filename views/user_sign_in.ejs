<h1 id="style">Nodejs Authentication App</h1>
<div id="login-box">
    <div class="left1">
      <h1>Sign In</h1>
      <form action="/users/create-session" method="POST">
      
      <input type="text" name="email" placeholder="E-mail*" required />
      <input type="password" name="password" placeholder="Password*" required />
      
      <span id="Req">* Required fields.</span>
      <input type="submit" value="SIGN IN" />
    </form>
    </div>
    
    <div class="right">
      <!-- <span class="loginwith">Sign in with<br />social network</span> -->
      <button class="social-signin google"><a href="/users/auth/google">Sign In with Google</a></button>
        <div id="signinn">
            <span>NEW USER? </span>
            <a href="/users/signup"><button class="btn2">SIGN UP</button></a>
        </div>

    </div>
    <div class="or">OR</div>

    
  </div>